{
    "name": "adaptly",
    "version": "1.0.0",
    "main": "source/server.ts",
    "repository": "https://github.com/supalarry/adaptly",
    "author": "Lauris <lauris.skraucis@gmail.com>",
    "engines": {
        "node": "^18.x",
        "pnpm": ">= 8.0.0"
    },
    "license": "MIT",
    "private": false,
    "scripts": {
        "dev": "doppler run -- nodemon --ignore ./tmp/ source/server.ts",
        "build": "pnpm clean && prettier --write source/ && babel source --out-dir build --extensions '.ts,.tsx' && pnpm build:prisma",
        "clean": "rm -rf build",
        "build:prisma": "prisma generate",
        "migrate:dev": "doppler run -- pnpm prisma migrate dev",
        "migrate:deploy": "doppler run -- pnpm prisma migrate deploy",
        "start": "pnpm migrate:deploy && doppler run -- node ./build/server.js",
        "test": "doppler run -- jest --testPathIgnorePatterns eval/",
        "eval-ts": "doppler run -- jest eval/isFileAffected.typescript.test.ts",
        "eval-py": "doppler run -- jest eval/isFileAffected.python.test.ts",
        "prisma-studio": "doppler run -- prisma studio",
        "typecheck": "tsc --noEmit"
    },
    "prisma": {
        "seed": "doppler run -- ts-node prisma/seed.ts"
    },
    "packageManager": "pnpm@8.6.6",
    "devDependencies": {
        "@octokit/webhooks-types": "^6.11.0",
        "@types/body-parser": "^1.19.2",
        "@types/cors": "2.8.12",
        "@types/express": "4.17.13",
        "@types/jest": "^29.5.1",
        "@types/jsonwebtoken": "^9.0.1",
        "@types/markdown-it": "^12.2.3",
        "@types/node": "^20.2.3",
        "@types/retry": "0.12.2",
        "jest": "^29.5.0",
        "nodemon": "2.0.12",
        "prettier": "2.4.1",
        "prisma": "4.16.1",
        "ts-jest": "^29.1.0",
        "ts-node": "10.2.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "4.4.3"
    },
    "dependencies": {
        "@axiomhq/axiom-node": "0.12.0",
        "@babel/cli": "7.17.10",
        "@babel/preset-env": "7.20.2",
        "@babel/preset-typescript": "7.22.5",
        "@iarna/toml": "2.2.5",
        "@octokit/auth-app": "^4.0.9",
        "@octokit/core": "^4.2.0",
        "@prisma/client": "4.16.1",
        "@sentry/node": "^7.57.0",
        "@types/semver": "^7.5.0",
        "axios": "^1.3.6",
        "babel-plugin-module-resolver": "4.1.0",
        "body-parser": "^1.20.2",
        "bottleneck": "2.19.5",
        "cors": "2.8.5",
        "express": "4.17.1",
        "fast-glob": "3.3.0",
        "jsonwebtoken": "^9.0.0",
        "markdown-it": "^13.0.1",
        "openai": "^3.3.0",
        "p-map": "6.0.0",
        "retry": "0.13.1",
        "rimraf": "5.0.1",
        "semver": "^7.5.3",
        "simple-git": "3.19.1",
        "tree-sitter": "^0.20.5",
        "tree-sitter-python": "^0.20.1",
        "ts-morph": "^18.0.0"
    }
}
